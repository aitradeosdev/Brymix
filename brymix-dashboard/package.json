{
  "name": "brymix-dashboard",
  "version": "1.0.0",
  "description": "Enterprise Brymix Propfirm Challenge Checker Dashboard",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "start:dev": "NODE_ENV=development npm run dev",
    "start:prod": "NODE_ENV=production npm start",
    "vercel-build": "npm run build",
    "test": "cd client && npm test",
    "setup": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "propfirm",
    "trading",
    "dashboard",
    "enterprise"
  ],
  "author": "Brymix",
  "license": "MIT"
}
